{
    "tasks": [
        {
            "type": "shell",
            "label": "make-task",
            "command": "make -j placer",
            "options": {
                "cwd": "${workspaceFolder}/thunder/build"
            },
            "dependsOn": "cmake-task"
        },
        {
            "type": "shell",
            "label": "cmake-task",
            "command": "cmake .. -DCMAKE_BUILD_TYPE=Debug",
            "options": {
                "cwd": "${workspaceFolder}/thunder/build"
            },
            "dependsOn": "mk-build-dir"
        },
        {
            "type": "shell",
            "label": "mk-build-dir",
            "command": "mkdir -p ./build",
            "options": {
                "cwd": "${workspaceFolder}/thunder"
            }
        },
        {
            "type": "shell",
            "label": "make-task2",
            "command": "make -j router",
            "options": {
                "cwd": "${workspaceFolder}/cyclone/build"
            },
            "dependsOn": "cmake-task2"
        },
        {
            "type": "shell",
            "label": "cmake-task2",
            "command": "cmake .. -DCMAKE_BUILD_TYPE=Debug",
            "options": {
                "cwd": "${workspaceFolder}/cyclone/build"
            },
            "dependsOn": "mk-build-dir2"
        },
        {
            "type": "shell",
            "label": "mk-build-dir2",
            "command": "mkdir -p ./build",
            "options": {
                "cwd": "${workspaceFolder}/cyclone"
            }
        }
    ],
    "version": "2.0.0"
}